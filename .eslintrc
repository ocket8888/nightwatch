{
  "extends": [
	"eslint:recommended"
  ],
  "parserOptions": {
	"ecmaVersion": 2018,
	"sourceType": "module",
	"ecmaFeatures": {
	  "jsx": false
	}
  },
  "env": {
	"mocha": true,
	"node": true
  },
  "rules": {
	"eqeqeq": [
	  "error",
	  "smart"
	],
	"no-extra-boolean-cast": 0,
	"quotes": [
	  "error",
	  "double"
	],
	"curly": [
	  "error",
	  "all"
	],
	"no-console": [
	  "error",
	  {
		"allow": [
		  "error"
		]
	  }
	],
	"no-debugger": 1,
	"semi": [
	  "error",
	  "always",
	  {
		"omitLastInOneLineBlock": true
	  }
	],
	"no-trailing-spaces": "error",
	"no-else-return": "error",
	"no-extra-bind": 0,
	"no-implicit-coercion": 0,
	"no-useless-call": "error",
	"no-return-assign": "error",
	"eol-last": "error",
	"no-unused-vars": "error",
	"no-extra-semi": "error",
	"comma-dangle": "warn",
	"no-underscore-dangle": 0,
	"no-lone-blocks": 0,
	"array-bracket-spacing": 2,
	"object-curly-spacing": 2,
	"brace-style": [
	  2,
	  "1tbs",
	  {
		"allowSingleLine": true
	  }
	],
	"comma-spacing": 2,
	"comma-style": 2,
	"key-spacing": 2,
	"one-var": [
	  "error",
	  "never"
	],
	"semi-style": [
	  "error",
	  "last"
	],
	"space-in-parens": [
	  "error",
	  "never"
	],
	"keyword-spacing": [
	  2,
	  {
		"before": true,
		"after": true
	  }
	],
	"padding-line-between-statements": [
	  "error",
	  {
		"blankLine": "always",
		"prev": "*",
		"next": "return"
	  }
	],
	"indent": [
	  "error",
	  "tab"
	],
	"prefer-const": [
	  "error"
	]
  },
  "globals": {
	"Promise": true,
	"Proxy": true,
	"Set": true,
	"Reflect": true,
	"element": "readonly",
	"by": "readonly",
	"expect": "readonly",
	"browser": "readonly",
	"Key": "readonly"
  }
}
